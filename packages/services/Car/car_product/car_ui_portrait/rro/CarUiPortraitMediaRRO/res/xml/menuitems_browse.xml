<MenuItems
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <MenuItem
        app:search="true"
        app:id="@+id/menu_item_search"/>
    <MenuItem
        app:settings="true"
        app:id="@+id/menu_item_setting"
        app:uxRestrictions="UX_RESTRICTIONS_NO_SETUP"/>
    <MenuItem
        app:id="@+id/menu_item_equalizer"
        app:title="@string/menu_item_sound_settings_title"
        app:carUiIcon="@drawable/ic_equalizer"/>
    <MenuItem
        app:id="@+id/menu_item_selector"
        app:title="@string/menu_item_app_selector_title"
        app:tinted="true"
        app:visible="false"/>
    <MenuItem
        app:id="@+id/menu_item_selector_with_source_logo"
        app:title="@string/menu_item_app_selector_title"
        app:tinted="false"
        app:visible="false"/>
</MenuItems>